<!DOCTYPE html>
<html>
  <head>
    <title>ZillowClone</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.credentials.google[:api_key]}" %>
    <%= javascript_include_tag 'application' %>
    <script>
      window.heroBanner = "<%= image_url('hero_banner2.jpg')%>"
      window.houseIcon = "<%= image_url('house_icon.png')%>"
      window.userIcon = "<%= image_url('user_icon.png')%>"
      window.spillow = "<%= image_url('spillow.png')%>"

    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
